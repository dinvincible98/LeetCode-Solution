// c++


class Solution {
public:
    string simplifyPath(string path) {
        string res, temp;
        vector<string> stk;
        stringstream ss(path);
        
        while (getline(ss,temp,'/'))
        {
            cout<<temp<<endl;
            if (temp=="" || temp==".")  continue;
            if (temp==".." && !stk.empty()) stk.pop_back();
            else if(temp != "..")   stk.push_back(temp);
            
        }
        
        for (auto& s:stk) res += '/'+ s;
        return res.empty()? "/":res;
    }
};
