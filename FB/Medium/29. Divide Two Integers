//C++:

class Solution {
public:
    int divide(int A, int B) {
        if (A==INT_MIN && B==-1)    return INT_MAX;
        
        int sign = A>0^B>0? -1:1;
        
        long a = labs(A), b =labs(B), cnt = 0;
        
        while(a >= b)
        {
            long temp = b, m = 1;
            while (temp<<1 <= a)
            {
                temp <<= 1;
                m <<= 1;
            }
            
            a -= temp;
            cnt += m;
        }
        
        return sign*cnt;
    }
};


// TC: O(logn)
// SC: O(1)
