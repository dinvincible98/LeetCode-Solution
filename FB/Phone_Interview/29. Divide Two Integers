//C++:

class Solution {
public:
    int divide(int A, int B) {
        if (A==INT_MIN && B==-1)   return INT_MAX;
        
        long a = labs(A), b = labs(B), res = 0;
        int sign = A>0^B>0 ? -1:1;
        while (a >= b)
        {
            long temp = b, m = 1;
            while (temp<<1 <= a)
            {
                temp <<= 1;
                m <<= 1;
            }
            a -= temp;
            res += m;
        }
        return sign * res;
        
    }
};

        


